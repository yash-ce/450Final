https://practice.geeksforgeeks.org/problems/palindrome-string0817/1

class Solution{
public:	
	
	
	int isPalindrome(string S)
	{
	    // Your code goes here
	    int n = S.size()-1;
	    for(int i=0;i<S.size()/2;i++){
	        if(S[i] != S[n--]){
	            return 0;
	        }
	    }
	    return 1;
	}

};
