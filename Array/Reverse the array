https://practice.geeksforgeeks.org/problems/reverse-a-string/1

//User function Template for C++

string reverseWord(string str){
    
  //Your code here
  string res = "";
  int n = str.size();
  for(int i=n-1;i>=0;i--){
      res += str[i];
    //   cout<<str[i]<< " y\n";
  }
//   cout << res << "yahs";
  return res;
}
